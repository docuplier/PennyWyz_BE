{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "func-names": 2,
        "global-require": 0,
        "import/no-dynamic-require": 0,
        "import/extensions": 0,
        "max-classes-per-file": 0,
        "class-methods-use-this": 0
    }
}
